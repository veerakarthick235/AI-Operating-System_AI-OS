{
  "name": "ai-os-ui-full",
  "version": "0.1.0",
  "main": "public/electron.js",
  "scripts": {
    "start": "concurrently \"npm:ui\" \"npm:electron\"",
    "ui": "cd renderer && npm start",
    "electron": "wait-on http://localhost:3000 && electron .",
    "build": "cd renderer && npm run build",
    "package": "electron-packager . ai-os-ui --platform=win32 --arch=x64 --out=dist --overwrite"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "wait-on": "^7.0.1",
    "electron": "^26.2.0",
    "electron-packager": "^17.1.1"
  }
}